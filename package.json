{
  "name": "automated-profit-system",
  "version": "1.0.0",
  "description": "Automated POD Profit System",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src src/server.js",
    "setup:team-profits": "node src/database/init-team-profits.js",
    "setup:payments": "node src/database/init-payments.js",
    "setup:onboarding": "node src/database/init-onboarding.js",
    "setup:multi-tenant": "node src/database/init-multi-tenant.js",
    "setup:all": "npm run setup:team-profits && npm run setup:payments && npm run setup:onboarding && npm run setup:multi-tenant",
    "setup:demo": "node scripts/setup-demo-account.js",
    "check:env": "node scripts/check-env.js",
    "check:env:required": "node scripts/check-env.js required",
    "check:env:production": "node scripts/check-env.js production",
    "check:env:info": "node scripts/check-env.js info",
    "generate:env": "node scripts/check-env.js generate",
    "vercel-build": "echo 'Build complete - using existing src'",
    "vercel:dev": "vercel dev",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.10.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "google-trends-api": "^4.9.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "sqlite3": "^5.1.6",
    "stripe": "^19.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "@vercel/node": "^3.0.0"
  },
  "engines": {
    "node": ">=18.x"
  }
}
